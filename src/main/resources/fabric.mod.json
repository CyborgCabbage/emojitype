{
  "schemaVersion": 1,
  "id": "emojitype",
  "version": "${version}",

  "name": "Emoji Type",
  "description": "A mod that lets you type emoji",
  "authors": [
    "CyborgCabbage"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/emoji-type-fabric",
    "sources": "https://github.com/CyborgCabbage/emojitype"
  },

  "license": "GPL-3.0",
  "icon": "assets/emojitype/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "cyborgcabbage.emojitype.EmojiTypeMod"
    ],
    "modmenu": [
      "cyborgcabbage.emojitype.config.EmojiTypeConfig"
    ]
  },
  "mixins": [
    "emojitype.mixins.json"
  ],

  "depends": {
    "fabricloader": "*",
    "minecraft": "1.17.1"
  },
  "suggests": {
    "cloth-config2" : "*"
  }
}
